{
    "name": "simonhamp/laravel-stripe-connect",
    "type": "library",
    "keywords": [
        "laravel",
        "stripe",
        "billing"
    ],
    "require": {
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "stripe/stripe-php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SimonHamp\\LaravelStripeConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimonHamp\\LaravelStripeConnect\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimonHamp\\LaravelStripeConnect\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "rap2h",
            "email": "raphaelht@gmail.com"
        },
        {
            "name": "Simon Hamp",
            "email": "simon.hamp@me.com"
        }
    ],
    "minimum-stability": "stable"
}
